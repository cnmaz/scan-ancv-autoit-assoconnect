	WinWaitActive("AssoConnect")
	Send($titre)
	Send("{TAB}")
	WinWaitActive("AssoConnect")
	Send($tarif)
	Send("{TAB 4}")
	WinWaitActive("AssoConnect")
	Send($debut)
	Send("{TAB}")
	WinWaitActive("AssoConnect")
	Send($fin)
	Send("{TAB 3}{SPACE}")
	Sleep(300)
	Send("{TAB 50}{SPACE}")
	Sleep(300)
	Send("{TAB 13}")
	Sleep(300)
	Send("{SPACE}")
	Sleep(300)
	Send("{TAB}708850")
	MsgBox(0,"Suite", "Suite")
EndFunc

